pipeline {
    agent any

    stages {
        stage('Clone Repository') {
            steps {
                // Clone your repo
                git branch: 'master', url: 'https://github.com/karthikreddy2250/my-automation-project.git'
            }
        }

        stage('Build') {
            steps {
                // Example: if it's a Java + Maven project
                sh 'mvn clean install'
            }
        }
    }
}
